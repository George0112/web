subtitle = '[{ "index": 2, "transcripts": [ { "t": "900", "d": "935", "text": "Teacher: Shh, shh. Settle, settle, please." }, { "t": "1868", "d": "3003", "text": "Eyes up here, eyes up here. We\'re not done." }, { "t": "4904", "d": "5606", "text": "Group three, let\'s have Ivy, and Michael," }, { "t": "10543", "d": "3036", "text": "uh, Ryan, Sally," }, { "t": "13612", "d": "2403", "text": "and... Thomas." }, { "t": "16048", "d": "1335", "text": "Thomas?" }, { "t": "17416", "d": "5606", "text": "Your homework is to explore [THUD] gravity." }, { "t": "23055", "d": "2003", "text": "[BELL RINGING] Don\'t forget," }, { "t": "25091", "d": "1802", "text": "projects are due on Friday." }, { "t": "26926", "d": "2102", "text": "Friday, please! Friday." }, { "t": "29061", "d": "1502", "text": "And don\'t forget your..." }, { "t": "30596", "d": "1368", "text": "[INDISTINCT CHATTER] ...homework." }, { "t": "31997", "d": "1536", "text": "♪ Music playing ♪" }, { "t": "33566", "d": "4438", "text": "Voiceover: Homework. Oh, homework." }, { "t": "38037", "d": "2069", "text": "I hate you." }, { "t": "40139", "d": "2002", "text": "You stink." }, { "t": "42174", "d": "2270", "text": "I wish I could wash you away in the sink." }, { "t": "44477", "d": "2336", "text": "Child: You guys ready? Children: Almost ready!" }, { "t": "46846", "d": "4204", "text": "Voiceover: If only a hippo would smash you to bits." }, { "t": "51083", "d": "2670", "text": "Homework. Oh, homework." }, { "t": "53786", "d": "1068", "text": "Child: Did we get it?" }, { "t": "54887", "d": "1535", "text": "Voiceover: Your giving me fits." }, { "t": "56455", "d": "3971", "text": "Children: Wow! We got it! That\'s amazing! Yeah!" }, { "t": "60459", "d": "3504", "text": "Voiceover: I\'d rather take baths with a man-eating shark," }, { "t": "63996", "d": "3337", "text": "or wrestle a lion alone in the dark," }, { "t": "67366", "d": "2670", "text": "eat spinach and liver," }, { "t": "70069", "d": "2503", "text": "pet 10 porc