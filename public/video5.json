subtitle = '[{ "index": 5, "transcripts": [ { "t": "4180", "d": "1620", "text": "A one point game" }, { "t": "15080", "d": "2000", "text": "Bryant for the win" }, { "t": "34900", "d": "1400", "text": "Dear Basketball" }, { "t": "42100", "d": "3020", "text": "From the moment I started rolling my dad\'s tube socks" }, { "t": "47600", "d": "3600", "text": "I shoot an imaginary game winning shot in a great western form" }, { "t": "53980", "d": "1780", "text": "I knew one thing was real" }, { "t": "56460", "d": "1600", "text": "I fell in love with You" }, { "t": "59560", "d": "2780", "text": "A love so deep I gave You my all" }, { "t": "63340", "d": "1480", "text": "From my mind and body" }, { "t": "65575", "d": "1540", "text": "To my Spirit and Soul" }, { "t": "69740", "d": "2600", "text": "As a 6 year old boy deeply in love with You" }, { "t": "76820", "d": "2260", "text": "I never saw an end at the tunnel" }, { "t": "79480", "d": "1840", "text": "I only saw myself running out of it" }, { "t": "85720", "d": "1020", "text": "And so I ran" }, { "t": "88460", "d": "3700", "text": "I ran up and down every court after every loose ball for You" }, { "t": "94220", "d": "1520", "text": "You asked for my Hustle" }, { "t": "97700", "d": "1480", "text": "I gave You my Heart" }, { "t": "102020", "d": "1780", "text": "Because I came for so much more" }, { "t": "107020", "d": "2360", "text": "I played through the sweat and the hurt" }, { "t": "114140", "d": "1920", "text": "Not because challenge called me" }, { "t": "119980", "d": "2140", "text": "But because You called me" }, { "t": "124520", "d": "740", "text": "I did everything" }, { "t": "125260", "d": "1280", "text": "FOR YOU" }, { "t": "128300", "d": "4360", "text": "Cause that\'s what you do when someone makes you feel as alive as You\'ve made me feel" }, { "t": "135220", "d": "2420", "text": "You gave a 6 year old boy his Laker Journey" }, { "t": "139200", "d": "1640", "text": "And I\'ll always love You for it" }, { "t": "143620", "d": "2860", "text": "I cant love You obsessively for much longer" }, { "t": "147080", "d": "2420", "text": "This season is all I have left to GIVE" }, { "t": "151120", "d": "2040", "text": "My HEART can take the pounding" }, { "t": "153380", "d": "2120", "text": "My MIND can handle the grind" }, { "t": "155780", "d": "2640", "text": "But my BODY knows it\'s time to say Goodbye" }, { "t": "160540", "d": "1800", "text": "That\'s Okay" }, { "t": "162600", "d": "1700", "text": "I\'m ready to let You go" }, { "t": "166880", "d": "1320", "text": "I want You to know now" }, { "t": "168240", "d": "900", "text": "So we both can savour" }, { "t": "169140", "d": "2180", "text": "Every moment we have left together" }, { "t": "173640", "d": "1920", "text": "The Good and the Bad" }, { "t": "176440", "d": "2280", "text": "We\'ve given each other all that we have" }, { "t": "179620", "d": "1260", "text": "And we both know" }, { "t": "182600", "d": "1240", "text": "No matter what I do next" }, { "t": "184480", "d": "2720", "text": "I\'ll always be that kid with the rolled up socks" }, { "t": "187500", "d": "1260", "text": "Garbage can in the corner" }, { "t": "189240", "d": "1280", "text": "5 seconds on the clock" }, { "t": "191360", "d": "1340", "text": "Ball in my hands" }, { "t": "193840", "d": "740", "text": "5" }, { "t": "194940", "d": "720", "text": "4" }, { "t": "195980", "d": "760", "text": "3" }, { "t": "197060", "d": "800", "text": "2" }, { "t": "199360", "d": "700", "text": "1" }, { "t": "225740", "d": "1440", "text": "Love You always" } ] }]';

